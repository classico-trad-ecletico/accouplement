@article{terreaux:2017pierre,
  title = {Pierre-Alexis Ponce de La Feuille, collectionneur de Poussin au Grand Siècle},
  author = {Terreaux, Clara},
  date = {2017},
  journaltitle = {Revue de l'art},
  pages = {37--54},
  issn = {0035-1326},
  url = {https://dialnet.unirioja.es/servlet/articulo?codigo=6238254},
  urldate = {2018-02-21},
  abstract = {Pierre-Alexis Ponce de La Feuille, Poussin Collecter in the Grand Siècle. In 1671, the "Comptes du Bâtiments du Roi" indicate a sale by a mysterious "La Feuille" of thirty-four paintings to Louis XIV: a major acquisition for the collections of the sovereign, indicative of the quantitative importance of the whole, the price (30,000 lt), and the quality of the paintings, including masterpieces by Rubens, Poussin, and Titian. The discovery of Pierre-Alexis Ponce de La Feuille's posthumous inventory unveils today the identity of the seller, financier, architect, and engineer working beside Clerville for the construction of the Canal du Midi. The document especially reveals the existence of an unpublished, exceptional collection of almost two hundred paintings, including among other major works, Pieter Brueghel's "The Beggars", Charles Le Brun's "Horatius Coclès" (fig. 5), seven paintings by Nicolas Poussin. The study of the relationships of Ponce de La Feuille allows us to examine his taste for the arts, but also to envisage a possible commerce of paintings implicating several financiers. His ties with Simon Lenoir, in particular, raise new hypotheses as to the destiny of the paintings of the collection of Nicolas Fouquet -among them, Nicolas Poussin's "La Manne" (fig. 7)- when they were dispersed in the 1660s.},
